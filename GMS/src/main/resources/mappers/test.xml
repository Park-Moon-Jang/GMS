   <insert id="insert" parameterType="com.goods.app.vo.TestVO" >
    INSERT INTO item_category (category_no, category_name) VALUES (#{category_no}, #{category_name})
   </insert>
   
   <select id="tListAll" resultType="com.goods.app.vo.TestVO">
   SELECT * FROM item_category ORDER BY category_no
   </select>
   
   <delete id="deleteOne" parameterType="int">
   delete from item_category where category_no = #{category_no}
   </delete>
   
 </mapper>
